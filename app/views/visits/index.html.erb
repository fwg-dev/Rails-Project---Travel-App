<h1>Places you've visited!</h1>

<% @visits.each do |visit| %>
<ul>
  <li><%= link_to visit.city.name, city_path(visit.city) %> | <%= link_to visit.city.country.name, country_path(visit.city.country)%> | <%= visit.city_rating %> stars</li>
  <%= link_to "Delete", [@user,visit], method: :delete %>
</ul>
<% end %>
